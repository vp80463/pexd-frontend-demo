{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "i18n-ally.localesPaths": ["src/i18n/locales"],
  "i18n-ally.pathMatcher": "locale_i18n_{locale}.{ext}",
  "i18n-ally.enabledParsers": [
    "yaml",
    "json"
  ],
  "i18n-ally.enabledFrameworks": [
    "vue",
    "vue-sfc"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ]
}
